{
  "name": "gems",
  "version": "1.0.0",
  "description": "A new concept product",
  "main": "/src/index.js",
  "scripts": {
    "start": "node -r @babel/register ./src",
    "watch": "nodemon --watch src -r @babel/register ./src/index.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZaHuPro/Gems.git"
  },
  "keywords": [
    "Gems",
    "bucket",
    "flow"
  ],
  "author": "ZaHuPro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZaHuPro/Gems/issues"
  },
  "homepage": "https://github.com/ZaHuPro/Gems#readme",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-transform-destructuring",
      "@babel/plugin-transform-object-assign",
      [
        "module-resolver",
        {
          "alias": {
            "@src": "./src",
            "@controllers": "./src/controllers",
            "@exception": "./src/exception",
            "@middlewares": "./src/middlewares",
            "@providers": "./src/providers",
            "@routes": "./src/routes",
            "@utils": "./src/utils"
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "axios": "^0.21.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-register": "^6.26.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-sanitizer": "^1.0.5",
    "express-useragent": "^1.0.13",
    "faker": "^4.1.0",
    "generate-password": "^1.4.2",
    "ipware": "^2.0.0",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-transform-destructuring": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  }
}
